{
  "env": {
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 6
  },
  "rules": {
    "no-unused-vars": 0,
    "no-undef": 0,
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"],
    "no-console": 0
  }
}
